{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:11": "react-native run-ios --simulator \"iPhone 11 Pro\"",
    "ios": "react-native run-ios",
    "jest": "jest --forceExit",
    "jest:watch": "jest --forceExit --detectOpenHandles --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pods": "npx pod-install",
    "postinstall": "yarn pods",
    "start": "react-native start",
    "test": "yarn typecheck && yarn lint && yarn jest",
    "tools:rename": "npx react-native-rename",
    "tools:set-icon": "react-native set-icon",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "react": "^16.13.1",
    "react-native": "^0.63.3",
    "react-native-device-info": "^6.0.1",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.7",
    "react-native-screens": "^2.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/runtime": "^7.8.4",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^3.3.0",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.12",
    "@types/react-native": "^0.63.13",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.8.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "typescript": "^4.0.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
